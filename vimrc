set nocompatible
syntax on
set number
set tabstop=4
set shiftwidth=4
set softtabstop=4
set expandtab
set autoindent
set hlsearch
set incsearch
set ignorecase
set smartcase
set noswapfile
set nobackup
set history=100
set clipboard=unnamedplus
filetype plugin indent on
set background=dark
colorscheme jellybeans
hi normal ctermbg=black
hi linenr ctermbg=black
hi endofbuffer ctermbg=black
