syntax on
set bg=dark
set mouse=a
set clipboard=unnamedplus
set softtabstop=4
set tabstop=4
set shiftwidth=4
set expandtab
set ai si
set hlsearch
hi Search ctermbg=red
let g:netrw_banner = 0
let g:netrw_liststyle = 3
let g:netrw_winsize = 25
nnoremap <Space>m :set nu!<CR>
nnoremap <Space>n :noh<CR>
nnoremap <Space>e :Lex<CR>
